@prefix : <#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gufo: <http://purl.org/nemo/gufo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mls: <http://www.w3.org/ns/mls#> .

# helper inverses

_:gufo_participant owl:inverseOf gufo:participatedIn .
_:gufo_created owl:inverseOf gufo:wasCreatedIn .

# specific ontology

:Preprocess rdf:type owl:Class ;
   rdfs:subClassOf mls:Task ,
      [ rdf:type owl:Restriction ;
         owl:onProperty _:gufo_participant ;
         owl:someValuesFrom :InputData
      ] ,
      [ rdf:type owl:Restriction ;
         owl:onProperty _:gufo_participant ;
         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         owl:onClass :PreprocessImplementation
      ] ,
      [ rdf:type owl:Restriction ;
         owl:onProperty _:gufo_created ;
         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
         owl:onClass :PreprocessedData
      ] .   